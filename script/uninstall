#!/bin/bash -e
#
# script/uninstall
# mas
#
# Removes mas and MasKit from PREFIX.
#

PREFIX=/usr/local

# Override default prefix path with optional 1st arg
if test -n "$1"; then
    PREFIX="$1"
fi

echo "==> ðŸ”¥ Uninstalling mas from $PREFIX"

trash "$PREFIX/Frameworks/MasKit.framework"
trash "$PREFIX/bin/mas"
